{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "AuthOption": {
    "Secret": "secret", //密钥，如果是JWT,必须>=16,客户端传过来client_secret的值 一定要跟这个相同,不然会出现 invalid_client
    "Authority": "http://localhost:5000", //发行者(颁发机构)(发布token的服务) ，token是谁颁发的
    "Audience": "api_longkc", //表示权限范围，与token的服务Scope一致
    "ExpiresIn": 10, //表示过期时间，单位为秒。如果省略该参数，必须其他方式设置过期时间。
    "ClientId": "longkc" //表示客户端的ID，必选项
  }
}
